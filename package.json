{
  "name": "svelte-fa-ts",
  "version": "0.9.0",
  "description": "Tiny FontAwesome 5 component for Svelte, with TypeScript types",
  "keywords": [
    "fontawesome",
    "font-awesome",
    "svelte",
    "svelte.js",
    "component",
    "icon",
    "svg",
    "duotone",
    "typescript"
  ],
  "scripts": {
    "prebuild": "rimraf lib/ && npm run validate",
    "build": "rollup -c",
    "validate": "svelte-check",
    "format": "prettier . --loglevel warn --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^1.1.11",
    "svelte-preprocess": "^4.5.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "browser": "lib/index.umd.js",
  "module": "lib/index.es.js",
  "main": "lib/index.cjs.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "dependencies": {},
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/svelte-fa-ts.git"
  },
  "bugs": {
    "url": "https://github.com/msanguineti/svelte-fa-ts/issues"
  },
  "homepage": "https://github.com/msanguineti/svelte-fa-ts#readme"
}
